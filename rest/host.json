{
    "version": "2.0",
    "concurrency": {
            "dynamicConcurrencyEnabled": true,
            "snapshotPersistenceEnabled": false
        },
    "extensions": {
        "http": {
            "routePrefix": "api"
        }
    },
    "extensionBundle": {
        "id": "Microsoft.Azure.Functions.ExtensionBundle",
        "version": "[4.0.0, 5.0.0)"
    },
    "functions": [ "CreateClient" ],
    "functionTimeout": "00:05:00",
    "logging": {
        "fileLoggingMode": "debugOnly",
        "logLevel": {
          "Function.CreateClient": "Information",
          "default": "Information"
        }
    }
}
